{
  "name": "feelcycle2cal",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/feelcycle-icon-16.jpg",
    "38": "images/feelcycle-icon-38.jpg",
    "128": "images/feelcycle-icon-128.jpg"
  },
  "default_locale": "ja",
  "background": {
    "service_worker": "scripts/background.js"
  },
  "host_permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.feelcycle.com/mypage*"
      ],
      "js": [
        "scripts/contentScript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "action": {
    "default_title": "feelcycle2cal"
  }
}
